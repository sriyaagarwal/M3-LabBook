<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:context="http://www.springframework.org/schema/context"
      xsi:schemaLocation="http://www.springframework.org/schema/beans 
      http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
            http://www.springframework.org/schema/context 
            http://www.springframework.org/schema/context/spring-context-4.3.xsd">
            
       <bean id="emp" class="com.cg.iter.Employee" autowire="constructor">     
     <constructor-arg name="employeeId" value="100"/>

  </bean>
  
  <bean id="empinfo" class="com.cg.iter.EmpInfo">
   <property name="empinfo1" >  
       <list>
         <value>Employee ID:100 </value>
       <value>Employee NAME:Rama</value>
       <value>Employee SALARY:12345.67</value>
       </list>
       </property>
      
  </bean>
</beans>